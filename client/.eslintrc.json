{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style" : "off",
        "operator-linebreak" : "off",
        "no-console" : "off",
        "no-use-before-define" : "off",
        "no-param-reassign" : "off",
        "destructuring-assignment": "off"
    }
}